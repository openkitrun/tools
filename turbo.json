{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "env": []
    },
    "generate": {
      "dependsOn": ["^generate"],
      "env": []
    },
    "lint": {
      "outputs": [],
      "cache": false
    },
    "lint:fix": {
      "outputs": [],
      "cache": false
    },
    "format": {
      "outputs": [],
      "cache": false
    }
  }
}
